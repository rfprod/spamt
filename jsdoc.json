{
	"tags": {
		"allowUnknownTags": true,
		"dictionaries": [ "jsdoc", "closure" ]
	},
	"opts": {
		"destination": "./logs/jsdoc"
	},
	"plugins": [
		"plugins/markdown",
		"jsdoc-http-plugin"
	],
	"templates": {
		"cleverLinks": false,
		"momospaceLinks": false,
		"default": {
			"outputSourceFiles": true
		},
		"path": "unk-docstrap",
		"theme": "flatly",
		"navType": "vertical",
		"linenums": true,
		"dateFormat": "MMMM DD YYYY, h:mm:ss a",

		"systemName"				: "SPAMT Server",
		"footer"						: "",
		"copyright"					: "",
		"includeDate"				: true,
		"navType"						: "vertical",
		"collapseSymbols"		: true,
		"inverseNav"				: false,
		"outputSourceFiles"	: true,
		"outputSourcePath"	: true,
		"syntaxTheme"				: "default",
		"sort"							: true,
		"search"						: true
	}
}
